/******************************************************************************

Find the 3rd Largest Number in an array

*******************************************************************************/
import java.util.*;
public class Main
{
    
    public static void findSecondLargest_WithBuildIn(int[] arr){
        Arrays.sort(arr);
        System.out.println(arr[arr.length-3]);
    }
    
    public static void findSecondLargest_WithoutBuildIn(int[] arr){
        if(arr.length<3){
            System.out.println("Size is less than 3");
        }
        int first=0,second=0,third=0;
        for(int i:arr){
            if(i>first){
                third = second;
                second = first;
                first=i;
            }
            else if(i>second && i!=first){
                third = second;
                second = i;
            }
            else if(i>third && i!=first && i!=second){
                third=i;
            }
            
        }
        System.out.println(third);
    }
    
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int[] arr = new int[n];
		for(int i=0;i<n;i++){
		    arr[i] = sc.nextInt();
		}
		findSecondLargest_WithBuildIn(arr);
		findSecondLargest_WithoutBuildIn(arr);
	}
}
